#include <stdio.h>

int main() {
    int a[10], i,j;

    printf("Enter the Array:\n");

    for (i = 0; i < 10; i++) {
        scanf("%d", &a[i]);
    }

    printf("The entered Array is: ");
    
    for (i = 0; i < 10; i++) {
        printf("%d ", a[i]); 
    }

	 for (j = 0; j < 10; j++) {
        scanf("%d", &a[j]);
    }

    printf("The entered Array is: ");
    
    for (j = 0; j < 10; j++) {
        printf("%d ", a[j]);
    }
    return 0;
}
