#include <stdio.h>

int main() {
    int a[2][2], i, j;

    printf("Enter the Elements of Array:\n");

    for (i = 0; i < 2; i++) {
        for (j = 0; j < 2; j++) {
            scanf("%d", &a[i][j]);
        }
    }

    printf("The entered Two-Dimensional Array:\n");
    for (i = 0; i < 2; i++) {
        for (j = 0; j < 2; j++) {
            printf("%d ", a[i][j]); 
        }
        printf("\n"); // Move to the next row
    }

    return 0;
}
